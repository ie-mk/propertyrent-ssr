{
  "name": "prop-rent",
  "version": "1.0.0",
  "author": "Justinas Simutis",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "next \"src/app/\" -p 8081",
    "dev-mock": "NODE_ENV=useMock next \"src/app/\" -p 8081",
    "preserve": "yarn build-public && yarn build-funcs && yarn build-app && yarn copy-deps && yarn install-deps",
    "serve": "cross-env NODE_ENV=production firebase serve",
    "predeploy": "yarn build-public && yarn build-funcs && yarn build-app && yarn copy-deps",
    "deploy": "NODE_ENV=production firebase deploy",
    "clean": "rimraf \"dist/functions/**\" && rimraf \"dist/public\"",
    "build-public": "cpx \"src/app/public/**/*.*\" \"dist/public\" -C",
    "build-funcs": "babel \"src/functions\" --out-dir \"dist/functions\"",
    "build-app": "next build \"src/app/\"",
    "copy-deps": "cpx \"*{package.json,package-lock.json,yarn.lock}\" \"dist/functions\" -C",
    "install-deps": "cd \"dist/functions\" && npm i",
    "build-api": "rimraf \"dist-api/**\" &&  NODE_ENV=build webpack --config webpack.api.config.js --progress",
    "cypress:open": "./e2e/node_modules/.bin/cypress open -P e2e",
    "cypress:openLocal": "./e2e/node_modules/.bin/cypress open -P e2e --env E2ELOCAL=true"
  },
  "dependencies": {
    "@kiwicom/orbit-components": "0.73.0",
    "@zeit/next-css": "1.0.1",
    "babel-plugin-styled-components": "1.10.7",
    "firebase": "7.1.0",
    "firebase-admin": "^8.9.0",
    "firebase-functions": "^2.1.0",
    "firebaseui": "^4.5.0",
    "formik": "2.1.4",
    "geofirex": "0.1.0",
    "google-map-react": "1.1.7",
    "google-maps-react": "2.0.2",
    "grpc": "1.22.2",
    "i18next": "19.4.4",
    "lodash.debounce": "^4.0.8",
    "next": "9.2.2",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-country-region-selector": "^2.0.0",
    "react-dates": "21.8.0",
    "react-dom": "16.8.6",
    "react-firebaseui": "4.1.0",
    "react-flags-select": "1.1.12",
    "react-geocode": "0.2.1",
    "react-google-maps": "9.4.5",
    "react-i18next": "^11.4.0",
    "react-multi-carousel": "2.5.5",
    "react-phone-number-input": "3.0.22",
    "react-redux": "6.0.1",
    "react-router": "3.2.4",
    "react-styled-carousel": "0.5.1",
    "react-telephone-input": "4.75.4",
    "redux": "4.0.4",
    "redux-actions": "2.6.5",
    "redux-form": "8.2.6",
    "redux-form-validators": "3.3.2",
    "redux-notifications": "4.0.1",
    "redux-saga": "1.1.3",
    "reduxsauce": "1.1.0",
    "reselect": "4.0.0",
    "rxjs": "6.5.5",
    "seamless-immutable": "7.1.4",
    "styled-components": "5.1.0",
    "webpack": "4.29.6",
    "webpack-bundle-analyzer": "3.1.0",
    "yup": "0.28.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "babel-eslint": "8.2.3",
    "babel-loader": "^8.1.0",
    "cpx": "^1.5.0",
    "cross-env": "^5.2.0",
    "cypress": "^4.5.0",
    "eslint": "5.16.0",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-react": "7.7.0",
    "firebase-functions-test": "^0.1.6",
    "firebase-tools": "^6.1.0",
    "husky": "^3.0.8",
    "prettier": "^1.19.1",
    "pretty-quick": "^1.11.1",
    "rimraf": "^3.0.2",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "eslintIgnore": [
    "**/*.plugin.js",
    "**/*.min.js"
  ]
}
